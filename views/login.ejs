<!DOCTYPE html>
<html>
<head>
  <title>creek</title>
  <link rel="stylesheet" href="stylesheets/style.css" type="text/css" media="screen">
  <link rel="stylesheet" href="stylesheets/icomoon.css" type="text/css" media="screen">
  <link rel="stylesheet" href="stylesheets/bootstrap.min.css" type="text/css" media="screen">
</head>
    <body>
        <script id="login-tmpl" type="text/html">
        <div>
            <form action="/login" class="well form-inline sigin" method="post">
                <div class="buttonHolder">
                    <span id="logo">
                        <i aria-hidden="true" style="font-size:25px;" class="icon-feather"></i>
                        creek
                    </span>
                </div>
                <div>
                    <input type="text" id="signin-email" class="input" name="username" title="Creek ID" tabindex="1"
                    placeholder="creek id" autocomplete="off">
                </div>
                <div>
                    <input type="password" id="signin-password" class="input" name="password" title="Password" tabindex="2"
                    placeholder="secret">
                </div>
                <div class="buttonHolder">
                    <button type="submit" class="submit btn primary-btn btn-success" id="go" tabindex="3">Sign in</button>
                </div>
            </form>
        </div>
        </script>
        <script type="text/javascript" charset="utf-8">
            if (window.location.pathname != '/') {
                window.location.pathname = window.location.pathname.slice(0, 0);
            };
        </script>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.4/underscore-min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/backbone.js/0.9.10/backbone-min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/mustache.js/0.7.2/mustache.min.js"></script>
        <script type="text/javascript" charset="utf-8">
            var App = {};
        </script>
        <script src="appjs/AppView.js" type="text/javascript" charset="utf-8"></script>
        <script src="appjs/User.js" type="text/javascript" charset="utf-8"></script>
        <script src="appjs/UserList.js" type="text/javascript" charset="utf-8"></script>
        <script src="appjs/UserView.js" type="text/javascript" charset="utf-8"></script>
        <script src="appjs/LoginView.js" type="text/javascript" charset="utf-8"></script>
        <script type="text/javascript" charset="utf-8">
            $(function () {
                App.creek = new App.LoginView( <%- JSON.stringify(process) %> );
            });
        </script>
    </body>
</html>